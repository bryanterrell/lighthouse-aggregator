{
  "name": "lighthouse-aggregator",
  "version": "1.0.0",
  "main": "main.js",
  "scripts": {
    "format": "prettier --write \"src/**/*.ts\" \"src/**/*.js\"",
    "lint": "tslint -p tsconfig.json",
    "build": "tsc",
    "test": "ts-node src/main.ts --action=test",
    "report": "ts-node src/main.ts --action=report",
    "compare": "ts-node src/main.ts --action=compare"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org/"
  },
  "devDependencies": {
    "@types/node": "^12.12.11",
    "prettier": "^1.19.1",
    "ts-node": "^8.5.2",
    "tslint": "^5.20.1",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.2"
  },
  "dependencies": {
    "chrome-launcher": "^0.12.0",
    "idx": "^2.5.6",
    "lighthouse": "^5.6.0",
    "minimist": "^1.2.0"
  }
}
